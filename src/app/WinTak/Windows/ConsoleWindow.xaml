<takui:StickyTakWindow x:Class="STak.WinTak.ConsoleWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:takui="clr-namespace:STak.WinTak"
        mc:Ignorable="d"
        Title="PowerShell Console" Height="800" Width="600">

    <ScrollViewer x:Name="m_scrollViewer">
        <StackPanel>
            <TextBox x:Name="m_console" AcceptsReturn="True" FontFamily="Courier New" FontSize="14" Margin="5">
            </TextBox>
        </StackPanel>
    </ScrollViewer>
</takui:StickyTakWindow>
